# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_API_TIMEOUT=30000

# Authentication (Google OAuth + Supabase)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI Model Endpoints
VITE_MODEL_ENDPOINT=http://localhost:5000/predict
VITE_GRADCAM_ENDPOINT=http://localhost:5000/gradcam

# File Upload
VITE_MAX_FILE_SIZE=104857600
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/dicom

# Feature Flags
VITE_ENABLE_DEVICE_MANAGER=true
VITE_ENABLE_PATIENT_PORTAL=true
VITE_ENABLE_EDUCATION=true

# Environment
VITE_ENV=development
